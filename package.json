{
  "name": "test-website",
  "version": "0.0.1",
  "description": "test-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf dist && yarn webpack && npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.css -c ./tailwind.config.cjs",
    "webpack": "webpack --config ./webpack.config.cjs",
    "tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.css -c ./tailwind.config.cjs",
    "dev:webpack": "webpack w --config ./webpack.config.cjs",
    "dev:tailwind": "npx tailwindcss -i ./src/tailwind.css -o ./dist/tailwind.css -w -c ./tailwind.config.cjs",
    "serve": "npx serve dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelnedev/test-website.git"
  },
  "author": "Joel N. Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joelnedev/test-website/issues"
  },
  "homepage": "https://github.com/joelnedev/test-website#readme",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "http-server": "^0.12.3",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.2.1",
    "tailwind-watch": "^0.0.6",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  }
